<?xml version="1.0"?>
<PropertyList>

    <path>gyro-H.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>Rollneedle</object-name>
        <property>/instrumentation/attitude-indicator/indicated-roll-deg</property>
        <interpolation>
            <entry><ind>-30.0</ind><dep>-30.0</dep></entry>
            <entry><ind>30.0</ind><dep>30.0</dep></entry>
        </interpolation>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>HzBar</object-name>
        <property>/instrumentation/attitude-indicator/indicated-roll-deg</property>
        <interpolation>
            <entry><ind>-30.0</ind><dep>-30.0</dep></entry>
            <entry><ind>30.0</ind><dep>30.0</dep></entry>
        </interpolation>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>HzBar</object-name>
        <property>/instrumentation/attitude-indicator/indicated-pitch-deg</property>
        <interpolation>
            <entry><ind>-30.0</ind><dep>0.0170</dep></entry>
            <entry><ind>30.0</ind><dep>-0.0170</dep></entry>
        </interpolation>
        <axis>
            <x>0.0</x>
            <y>0.0</y>
            <z>1.0</z>
        </axis>
    </animation>

</PropertyList>
