<?xml version="1.0"?>

<PropertyList>

<path>bmep2.ac</path>

    <animation>
        <type>material</type>
        <object-name>BMEP2.face</object-name>
        <object-name>BMEP2.needle</object-name>
        <emission>
            <red>0.8</red>
            <green>0.4</green>
            <blue>0.2</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>BMEP2.needle</object-name>
        <property>/engines/engine[0]/bmep</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>60</ind><dep>22.0</dep></entry>
            <entry><ind>254</ind><dep>343</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>
