<?xml version="1.0"?>

<PropertyList>

<path>LHFflow.ac</path>

    <animation>
        <type>material</type>
        <object-name>LHFflow.face</object-name>
        <object-name>LHFflow.needle1</object-name>
        <object-name>LHFflow.needle2</object-name>
        <emission>
            <red>0.8</red>
            <green>0.4</green>
            <blue>0.2</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>LHFflow.needle1</object-name>
        <property>engines/engine[0]/fuel-flow-gph</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>50.0</ind><dep>9.0</dep></entry>
            <entry><ind>66.66</ind><dep>22.0</dep></entry>
            <entry><ind>416.5</ind><dep>275</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>LHFflow.needle2</object-name>
        <property>engines/engine[1]/fuel-flow-gph</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>50.0</ind><dep>9.0</dep></entry>
            <entry><ind>66.66</ind><dep>22.0</dep></entry>
            <entry><ind>416.5</ind><dep>275</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>