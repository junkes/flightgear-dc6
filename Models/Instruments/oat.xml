<?xml version="1.0"?>

<PropertyList>

<path>oat.ac</path>

    <animation>
        <type>material</type>
        <object-name>OAT.face</object-name>
        <object-name>OAT.needle</object-name>
        <emission>
            <red>0.8</red>
            <green>0.4</green>
            <blue>0.2</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>OAT.needle</object-name>
        <property>/environment/temperature-degf</property>
        <interpolation>
            <entry><ind>-60.0</ind><dep>0.0</dep></entry>
            <entry><ind>0</ind><dep>33.0</dep></entry>
            <entry><ind>120</ind><dep>99.5</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>-0.0245</z-m>
        </center>
    </animation>

</PropertyList>
