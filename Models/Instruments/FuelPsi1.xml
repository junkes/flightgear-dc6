<?xml version="1.0"?>

<PropertyList>

<path>fuelpsi.ac</path>

    <animation>
        <type>material</type>
        <object-name>FuelPsi.face</object-name>
        <object-name>FuelPsi.needle1</object-name>
        <object-name>FuelPsi.needle2</object-name>
        <emission>
            <red>0.8</red>
            <green>0.4</green>
            <blue>0.2</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FuelPsi.needle1</object-name>
        <property>engines/engine/running</property>
        <factor>70.0</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.024</y-m>
            <z-m>0.00</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FuelPsi.needle2</object-name>
        <property>engines/engine[1]/running</property>
        <factor>70.0</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0.024</y-m>
            <z-m>0.00</z-m>
        </center>
    </animation>

</PropertyList>