<!--Engines-->
<!-- ENG 1-->
    <animation>
        <type>select</type>
        <object-name>prop1</object-name>
        <condition>
            <less-than>
                <property>engines/engine[0]/rpm</property>
                <value>300.000</value>
            </less-than>
        </condition>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>prop1</object-name>
        <property>engines/engine[0]/rpm</property>
        <center>
            <x-m>-4.460</x-m>
            <y-m>-8.080</y-m>
            <z-m>0.265</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>select</type>
        <object-name>propblur1</object-name>
        <condition>
            <greater-than>
                <property>engines/engine[0]/rpm</property>
                <value>299.999</value>
            </greater-than>
            <less-than>
                <property>engines/engine[0]/rpm</property>
                <value>700</value>
            </less-than>
        </condition>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>propblur1</object-name>
        <property>engines/engine[0]/rpm</property>
